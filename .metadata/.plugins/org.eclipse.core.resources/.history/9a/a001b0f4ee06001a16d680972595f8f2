package tiposdedatos;

public class Taller1 {
	
	public static void main(String[] args) {
		
		int a=10;
		double b=20.3;
		double c=3.14785;
		
		System.out.println("Hola Mundo");
		System.out.println("++++++++++ \n@@@@@@@@ \n***********  \n########");
		System.out.println(a +  " " + b + " " +c);
	}

}
